LIBRARY ieee;
USE ieee.std_logic_1164.all;
USE ieee.numeric_std.all; 
USE ieee.std_logic_unsigned.all;

ENTITY SLLESQUERDA IS 
	PORT(
		VALOR: IN SIGNED(15 DOWNTO 0);
		SAIDA: OUT SIGNED(15 DOWNTO 0)
	);
END SLLESQUERDA;

ARCHITECTURE BEHAVIOR OF SLLESQUERDA IS
	BEGIN 
		SAIDA <= SLL(VALOR);
END BEHAVIOR;